{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://momenic.webinvit.id/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS" },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Accept, User-Agent, Content-Type"
        }
      ]
    }
  ]
}
